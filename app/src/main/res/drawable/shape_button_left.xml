<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid
                android:color="#1B5E20" />
            <corners
                android:topLeftRadius="20dp"
                android:bottomLeftRadius="20dp"/>
